<form class="form-inline my-2 my-lg-0" action="/user/signup" method="get">
<% if (loggedin == false) {%>
<input class="form-control mr-sm-2" type="text" name="username" placeholder="Username" autocomplete="username"/>
<input class="form-control mr-sm-2" type="password" name="password" placeholder="Password" autocomplete="current-password"/>
<input class="btn btn-outline-success my-2 my-sm-0" type="Submit" value="Login"/>
<%} else { %>
<input class="btn btn-outline-danger my-2 my-sm-0" type="Submit" value="Logout"/>
<%} %>
</form>
